{
  "store.search": {
    "parent": {
      "header": "header-layout.desktop",
      "footer": "footer-layout.desktop"
    },
    "blocks": [
      "search-result-layout"
    ]
  },
  "search-result-layout": {
    "blocks": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [ 
      "breadcrumb.search",
      "search-title.v2",      
      "search-fetch-previous", 
      "flex-layout.row#custom-top",
      "flex-layout.row#custom-layout",
      "search-fetch-more"
    ]
  },
  "flex-layout.row#custom-top": {
    "props": {
      "blockClass": "top-content"
    },
    "children": [ 
      "total-products.v2",
      "order-by.v2"
    ]
  },
  "flex-layout.row#custom-layout": {
    "props": {
      "preventHorizontalStretch": true,
      "fullWidth": true,
      "blockClass": "custom-filter"
    },
    "children": [
      "filter-navigator.v3",
      "flex-layout.row#product-content"
    ]
  },
  "flex-layout.row#product-content": {
    "props": {
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "fullWidth": true
    },
    "children": [
      "search-content"
    ]
  }
}